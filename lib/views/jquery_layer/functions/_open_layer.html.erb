function() {
  <%= layer.namespace %>['url'] = $(this).attr('href');
  $("<%= layer.layer_selector.html_safe %>").dialog(<%= layer.to_json %>);
  <%- if layer.destroy_on_exit -%>
  $("<%= layer.layer_selector.html_safe %>").bind("dialogclose", function(event, ui){debugger; $(this).children().detach();});
  <%- end -%>
}